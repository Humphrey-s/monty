#include <unistd.h>
#include <stdlib.h>
#include "monty.h"
#include <unistd.h>
/**
 * push - pushes an element to stack
 *
 * @stack: the stack
 * @line_number: line of instruction
 * Return: (Success)
 */
void _push(stack_t **stack, unsigned int line_number)
{
	stack_t *new_node = malloc(sizeof(stack_t));
	stack_t *ptr;

		new_node->n = element;
		new_node->next = NULL;
	
	if (head == NULL)
	{
		new_node->prev = head;
	
		head = new_node;
	}
	else
	{
		ptr = head;
	
		while (ptr->next != NULL)
		{
			ptr = ptr->next;
		}

		new_node->prev = ptr;
		ptr->next = new_node;
	
	}
}
/**
 * print_stack - prints elements in a stack
 * @stack: the stack
 * @line_number: line number
 * Return: (Success)
 */
void print_stack(stack_t **stack, unsigned int line_number)
{
	/*printf("hallo\n");*/

	stack_t *ptr = head;

	while (ptr != NULL)
	{
		printf("%d\n", ptr->n);
		ptr = ptr->next;
	}
}
